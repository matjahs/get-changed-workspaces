    "@yarnpkg/core": "^3.1.0",
    "@yarnpkg/fslib": "^2.6.0",
    "pkg-up": "^4.0.0",
    "set-value": ">=4.1.0",
    "tmpl": ">=1.0.5"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.2",
    "@typescript-eslint/parser": "^4.32.0",
    "@vercel/ncc": "^0.31.1",
    "eslint": "^8.5.0",
    "eslint-plugin-github": "^4.3.2",
    "eslint-plugin-jest": "^25.7.0",
    "fs-jetpack": "^4.2.0",
    "husky": "^7.0.4",
    "jest": "^27.4.5",
    "jest-circus": "^27.2.5",
    "jest-extended": "^1.2.0",
    "js-yaml": "^4.1.0",
